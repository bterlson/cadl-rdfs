import "cadl-rdf";

using CadlRdf;

@rdfns("ex", "http://example.org/")

namespace Resources;

@doc("a person")
@summary("person summary")
model Person {
  @doc("Name prop of person")
  @minLength(2)
  name: string;
  age: int8;
  vehicles: Vehicle[];
}

@doc("any kind of vehicle")
model Vehicle {
  owner: Person;
}

model Test1{
  model1: "My Model 1";
}

model Test2{
  model2: "My Model 2";
}

model myModel{
  ...Test1,
  ...Test2
}


@doc("a vehicle that moves people")
model Car extends Vehicle {
  @summary("color summary")
  color: "red" | "blue";
  myModel: myModel;
}

@doc("a truck for transporting cargo")
model Truck extends Vehicle {
  @doc("how many cubic feet of space")
  cargoSpace: int32;
}

@format("0378331000")
@doc("a nine digit number")
model CUSIP is string;